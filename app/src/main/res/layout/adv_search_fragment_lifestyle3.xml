            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="5dp"
            android:background="#d7d7d7"></View>

        <com.chicsol.alfalah.widgets.mTextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="7dp"
            android:layout_marginTop="10dp"
            android:text="Want Children"


            android:textColor="@color/colorUserProfileRed"
            android:textSize="@dimen/text_size_adv_search_headings"
            android:textStyle="bold" />

        <LinearLayout
            android:id="@+id/LinearLayoutAdvSearchWantChildren"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

        </LinearLayout>


        <View
